/*投注头部返回按钮*/

.bet-header {
    height: 70rpx;
    padding: 20rpx
}

.back-btn {
    background-color: #fff;
    height: 66rpx;
    border: 1rpx solid #c9cbca;
    box-shadow: 0px 0px 5px #c9cbca;
    border-radius: 8rpx;
    color: #fa8508;
    line-height: 66rpx;
    font-size: 26rpx;
    text-align: center
}


/*单关和竞彩的刷新*/

.refresh {
    height: 54rpx;
    background-color: #ebf5ff;
    font-size: 24rpx;
    padding-left: 115rpx;
    color: #1e56a3;
    position: fixed;
    left: 0;
    top: 0;
    width: 100%
}

.refresh span {
    color: #4e94ec;
    line-height: 54rpx;
    margin: 0 10rpx
}