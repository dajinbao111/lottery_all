/* 提示 */
.pick_numbers .hint {
    padding: 0 24rpx;
    color: #909493;
    line-height: 48rpx;
    margin: 10rpx 0;
}
/* 百、十、个位 */
.list_title {
    text-align: center;
    color: #b5b9b8;
    font-size: 32rpx;
}
/* 数字 */
.number_list {
    width: 704rpx;
    margin: 0 auto;
    height: 200rpx;
    padding: 30rpx 0;
    display: flex;
    flex-flow: row wrap;
}
.list_title text {
    color: #5f6362;
}
.number_list .num_btn {
    width: 68rpx;
    height: 68rpx;
    border: 1px solid #eaeceb;
    box-shadow: 1px 2px 5px rgba(32, 32, 32, .2);
    border-radius: 50%;
    text-align: center;
    line-height: 68rpx;
    margin-left: 16rpx;
    background-color: #fff;
    color: #d31e31;
}