/* public style */
page,.container{
  width: 100%;
  height: 100%
}
page{
  background-color: #f3f6f5;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #262626;
}
/* list */
.noTicks_list{
  background-color: #fff;
  border-bottom: 1px solid #f2f2f2;
  padding: 30rpx 100rpx 30rpx 40rpx;
  display: flex;
  justify-content: space-around
}
.noTicks_list:last-of-type{
  border-bottom: 1px solid transparent;
}
.noTicks_list:nth-of-type(even){
  background-color: #fbfbfb
}