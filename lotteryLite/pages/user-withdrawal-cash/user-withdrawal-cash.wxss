page {
    background-color: #fafafa;
}

.container {
    background: #fafafa;
    font-family: "微软雅黑";
    font-size: 28rpx;
}

.withdrawal_account {
    height: 216rpx;
    border-top: 1px solid #e2e1e1;
    border-bottom: 1px solid #e2e1e1;
    background: #fff;
    padding-left: 24rpx;
}

.getMoney,
.myAccount {
    height: 108rpx;
    line-height: 108rpx;
    position: relative;
    color: #202020;
    text-indent: 80rpx;
}

.myAccount {
    border-bottom: 1px solid #f3f3f3;
}

.myAccount image {
    width: 46rpx;
    height: 46rpx;
    position: absolute;
    left: 0;
    top: 32rpx;
}

.myAccount .money {
    position: absolute;
    right: 120rpx;
}

.myAccount .money text {
    color: #ff5534;
    font-weight: bold
}

.getMoney image {
    width: 56rpx;
    height: 38rpx;
    position: absolute;
    left: 0;
    top: 32rpx;
}

.getMoney .how_much {
    width: 310rpx;
    padding-left: 60rpx;
    height: 108rpx;
    line-height: 108rpx;
    float: left;
    text-align: left;
    text-indent: 0;
    font-size: 32rpx;
    font-family: "微软雅黑";
}

.how_much {
    text-align: right !important
}


/* 添加银行卡 */

.add_bank_cards {
    margin-top: 12rpx;
    margin-bottom: 40rpx;
}

.add_bank_cards .title {
    height: 88rpx;
    line-height: 88rpx;
    position: relative;
    border-top: 1px solid #e2e1e1;
    border-bottom: 1px solid #f3f3f3;
    text-indent: 104rpx;
    font-size: 30rpx;
    background: #fff;
}

.add_bank_cards .title image {
    width: 56rpx;
    height: 38rpx;
    position: absolute;
    left: 24rpx;
    top: 27rpx;
}

.no_bankcards_wrap {
    height: 100rpx;
    line-height: 100rpx;
    padding: 40rpx 30rpx 40rpx 30rpx;
    display: flex;
    align-items: center;
    position: relative;
    border-bottom: 1px solid #e2e1e1;
    background-color: #f6f9f6
}

.no_bankcards_text {
    color: #8c8b8b;
    line-height: 80rpx;
    border: 1px dashed #cccbcb;
    text-align: center;
    border-radius: 8rpx;
}

.sure_btn,
.cash_recodes {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    text-align: center;
    font-size: 32rpx;
}

.sure_btn {
    background: #fb592d;
    color: #fff;
    width: 702rpx
}

.cash_info_btn {
    margin-top: 40rpx;
}

.cash_recodes {
    background: #f2f2f2;
    color: #797979;
    margin-top: 32rpx;
}

.my_bankcards_list {
    width: auto;
    height: 100rpx;
    line-height: 100rpx;
    padding: 40rpx 30rpx 40rpx 30rpx;
    display: flex;
    align-items: center;
    position: relative;
    border-bottom: 1px solid #e2e1e1;
    background: #eceee8
}

.have_bankCard {
    width: 100%;
    background: #f15843;
    color: #fff;
    border-radius: 12rpx;
}

.bank_names {
    width: 30%;
    text-align: center
}

.bank_num {
    width: 70%;
    text-align: center
}

.no_bankcards {
    width: 100%;
}


